<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>zot-plus-plus@rnicrosoft-studio.github.io</em:id>
    <em:type>2</em:type> <!-- type=extension -->
    <em:name>Zot Plus Plus</em:name>
    <em:version>0.0.1</em:version>
    <em:creator>rnicrosoft-studio</em:creator>
    <em:description>Zotero++ (ZotPP), Zotero enhancement plugin by rnicrosoft-studio</em:description>
    <em:homepageURL>https://github.com/rnicrosoft-studio/zot-plus-plus</em:homepageURL>
    <em:unpack>true</em:unpack>
    
    <!-- Zotero -->
    <em:targetApplication>
      <Description>
        <em:id>zotero@chnm.gmu.edu</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>5.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    <!-- Juris-M, which is based on Zotero -->
